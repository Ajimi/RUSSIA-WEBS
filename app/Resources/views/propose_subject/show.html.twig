{% extends 'base.html.twig' %}

{% block body %}
    <h1>Propose_subject</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ propose_Subject.id }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{% if propose_Subject.etat %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('propose_subject_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('propose_subject_edit', { 'id': propose_Subject.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
