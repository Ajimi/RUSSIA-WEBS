            <div class="title_left">
                <h3>Players
                    <small>Stats</small>
                </h3>
            </div>

        </div>
    {% endblock %}
    {% block page_content %}
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content">
                    <div id="div_chart"></div>
                </div>
            </div>
        </div>
    {% endblock %}



